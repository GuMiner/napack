      <add assembly="System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <add assembly="Nancy" />
	  <add assembly="Napack.Common" />
	  <add assembly="Napack.Analyst" />
    </assemblies>
    <namespaces>
      <add namespace="System" />
      <add namespace="Nancy" />
      <add namespace="Nancy.ViewEngines.Razor" />
    </namespaces>
  </razor>
  <runtime>
