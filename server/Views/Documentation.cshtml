@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Napack.Server.DocumentationModel>
@model Napack.Server.DocumentationModel

@{
    Layout = "RootLayout.cshtml";
    string pageName = "Documentation";
}

@section Title {@pageName -- @Model.Name}

<div id="body">
    <div class="titleFlag" style="width: 200px;"></div>
    <h2 class="titleFlagCompensation">@pageName</h2>
    <div class="titleFlag" style="width: 210px;"></div>
    <h2 class="titleFlagCompensationSecondStyle"><i>@Model.Name</i></h2>

    @Html.Raw(@Model.Html).
</div>

@section Scripts{
    <script type="text/javascript">
        // Run the scripts.
        $(document).ready(function () {
        });
    </script>
}